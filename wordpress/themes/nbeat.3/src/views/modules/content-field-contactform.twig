{% extends "modules/content-field.twig" %}

{% block fieldContent %}
  <div class="content-section content-contactform">
    <div class="container">
      <div class="row">
        <div class="col-lg-10 col-lg-offset-1">

          {% if field.getHeading() %}
            <h2 class="content-two-thirds">{{field.getHeading()}}</h2>
          {% endif %}

          {{field.getForm()}}
          
        </div>
      </div>
    </div>
  </div>
{% endblock %}
