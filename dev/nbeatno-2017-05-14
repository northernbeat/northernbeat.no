/*!40000 ALTER TABLE `wp2015_postmeta` DISABLE KEYS */;
INSERT INTO `wp2015_postmeta` VALUES (1,2,'_wp_page_template','default');
/*!40000 ALTER TABLE `wp2015_postmeta` ENABLE KEYS */;
